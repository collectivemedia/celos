<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
        "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>

    <servlet>
        <servlet-name>UIServlet</servlet-name>
        <servlet-class>com.collective.celos.old.UIServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>UIRerunServlet</servlet-name>
        <servlet-class>com.collective.celos.old.UIRerunServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>VersionServlet</servlet-name>
        <servlet-class>com.collective.celos.servlet.VersionServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>WorkflowGroupsServlet</servlet-name>
        <servlet-class>com.collective.celos.ui.WorkflowGroupsServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ActionServlet</servlet-name>
        <servlet-class>com.collective.celos.ui.ActionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UIServlet</servlet-name>
        <url-pattern>/ui</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UIRerunServlet</servlet-name>
        <url-pattern>/rerun</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>VersionServlet</servlet-name>
        <url-pattern>/version</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>WorkflowGroupsServlet</servlet-name>
        <url-pattern>/groups</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ActionServlet</servlet-name>
        <url-pattern>/multiplex-rpc</url-pattern>
    </servlet-mapping>

</web-app>
