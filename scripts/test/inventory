celos_defaults_dir=~{{ service_user }}/celos_wrk/defaults
celos_ui_dir=~{{ service_user }}/celos_wrk/ui
celos_db_dir=~{{ service_user }}/celos_wrk/db
celos_log_dir=~{{ service_user }}/celos_wrk/logs

[test-server:vars]
