service_port=9002
jar_file="celos-0.1.jar"
service_args="--workflowsDir {{ celos_workflows_dir }} --defaultsDir {{ celos_defaults_dir }} --stateDir {{ celos_db_dir }} --uiDir {{ celos_ui_dir }} --logDir {{ celos_log_dir }}"
